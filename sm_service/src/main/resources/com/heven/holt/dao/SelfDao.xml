<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.heven.holt.dao.SelfDao">

    <select id="selectByAccount" parameterType="String" resultMap="com.heven.holt.dao.StaffDao.resultMap">
        SELECT *
        FROM staff
        WHERE account = #{account}
    </select>
</mapper>